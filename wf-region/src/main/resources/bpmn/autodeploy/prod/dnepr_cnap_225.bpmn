    <scriptTask id="scripttask1" name="Script Task" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>
var sum = 15.78;
if ((execution.getVariable('asLust')) === 'sEnamLustKur') {
	sum =  36.14;
}
execution.setVariable('sSum', sum);      
      
var gal = 'X'
var bs = ' '
execution.setVariable('iTemp1', bs)
execution.setVariable('iTemp2', bs)
execution.setVariable('iTemp3', bs)
if(execution.getVariable('aDocErr') == 'enum1'){execution.setVariable('iTemp1', gal)}
if(execution.getVariable('aDocErr') == 'enum2'){execution.setVariable('iTemp2', gal)}
if(execution.getVariable('aDocErr') == 'enum3'){execution.setVariable('iTemp3', gal)}
</script>
    </scriptTask>
