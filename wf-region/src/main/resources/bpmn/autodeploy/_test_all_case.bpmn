    <sequenceFlow id="flow1" name="смс с файлом" sourceRef="exclusivegateway1" targetRef="servicetask1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${decision=='accept'}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="servicetask1" name="Повідомлення про успішну реєстрацію звернення" activiti:delegateExpression="#{MailTaskWithAttachmentsAndSMS}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${email}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:expression><![CDATA[iGov: Тестовый]]></activiti:expression>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression><![CDATA[[pattern/mail/new_design/_common_header.html]
			[pattern/mail/new_design/_common_content_start.html]
			<h1>Тестовое письмо</h1>
			[pattern/mail/new_design/_common_content_end.html] 
			[pattern/mail/new_design/_common_signature_start.html]
			[pattern/mail/new_design/_common_signature_end.html]
			[pattern/mail/new_design/_common_footer.html]]]></activiti:expression>
        </activiti:field>
        <activiti:field name="sPhone_SMS">
          <activiti:expression><![CDATA[${phone}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="sText_SMS">
          <activiti:expression><![CDATA[iGov: tets]]></activiti:expression>
        </activiti:field>
        <activiti:field name="saAttachmentsForSend">
          <activiti:expression><![CDATA[" "]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow3111" sourceRef="servicetask1" targetRef="usertask1"></sequenceFlow>
    <userTask id="usertask1" name="Крок 1. Перевірка тестових даних" activiti:candidateGroups="tester">
      <extensionElements>
        <activiti:formProperty id="slFIO" name="Автор звернення" type="string" default="${bankIdlastName} ${bankIdfirstName} ${bankIdmiddleName}" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdPassport" name="Паспортні дані" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="phone" name="Контактний  телефон" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="email" name="Е-mail" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="asDecide1" name="Рішення про надання послуги" type="enum" required="true">
