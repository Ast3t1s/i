    <scriptTask id="scripttask3" name="Скрипт для печати 2" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>
var per1=""
var src=execution.getVariable('lastName')
for (var i = 0; i&lt;50; i++){
var day=src.substr(i,1)
if(day != "null" || day != ""){
if(day != "-" || day != "`" || day != " " || day != "'"){
per1 = per1+day}
else {
per1 = per1+"&lt;"}}
else{
break}}
execution.setVariable('sMoz1', per1)
var numb=execution.getVariable('sNumber')
var ser=numb.substr(0,2)
var num=numb.substr(2,6)
var a1=Number(numb.substr(2,1))
var a2=Number(numb.substr(3,1))
var a3=Number(numb.substr(4,1))
var a4=Number(numb.substr(5,1))
var a5=Number(numb.substr(6,1))
var a6=Number(numb.substr(7,1))
var k1=(10*7+11*3+a1*7+a2*3+a3*1+a4*7+a5*3+a6*1)%10
var src=execution.getVariable('sDate_of_birth1')
var day1=src.substr(0,2)
var month1=src.substr(3,2)
var year1=src.substr(8,2)
var b1=Number(year1.substr(0,1))
var b2=Number(year1.substr(1,1))
var b3=Number(month1.substr(0,1))
var b4=Number(month1.substr(1,1))
var b5=Number(day1.substr(0,1))
var b6=Number(day1.substr(1,1))
var k2=(b1*7+b2*3+b3*1+b4*7+b5*3+b6*1)%10
var src=execution.getVariable('sSex')
var sex=src.substr(4,1)
var src=execution.getVariable('sDate_of_issue')
var day2=src.substr(0,2)
var month2=src.substr(3,2)
var year2=src.substr(8,2)
var year4=Number(year2)+5
var year3='year4'
var c1=Number(year3.substr(0,1))
var c2=Number(year3[1])
var c3=Number(month2.substr(0,1))
var c4=Number(month2.substr(1,1))
var c5=Number(day2.substr(0,1))
var c6=Number(day2.substr(1,1))
k3=(c1*7+c2*3+c3*1+c4*7+c5*3+c6*1)%10
var moz2=ser+"&lt;"+num+k1+"UKR"+year1+month1+day1+k2+sex+year4+month2+day2+k3+"&lt;"+"&lt;"+"&lt;"+"&lt;"+"&lt;"+"&lt;"+"&lt;"+"&lt;"+"&lt;"+"&lt;"+"&lt;"+"&lt;"+"&lt;"+"&lt;"+"&lt;"
execution.setVariable('sMoz2', moz2)
var src=execution.getVariable('sDate_of_issue')
var day=src.substr(0,2)
var month=src.substr(3,2)
var year=src.substr(6,4)
var month1='СІЧ / JAN'
if (month=='02'){
    month1='ЛЮТ / FEB'}
if (month=='03'){
    month1='БЕР / MAR'}
if (month=='04'){
    month1='КВІ / APR'}
if (month=='05'){
    month1='ТРА / MAY'}
if (month=='06'){
    month1='ЧЕР / JUN'}
if (month=='07'){
    month1='ЛИП / JUL'}
if (month=='08'){
    month1='СЕР / AUG'}
if (month=='09'){
    month1='ВЕР / SEP'}
if (month=='10'){
    month1='ЖОВ / OCT'}
if (month=='11'){
    month1='ЛИС / NOV'}
if (month=='12'){
    month1='ГРУ / DEC'}
var issue=day+' '+month1+' '+year
var year1= year-(-5)
var expiry=day+' '+month1+' '+year1
execution.setVariable('sDate_of_issue', issue) 
execution.setVariable('sDate_of_expiry', expiry)
       </script>
    </scriptTask>
