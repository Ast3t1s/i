<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:activiti="http://activiti.org/bpmn" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC" xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI" typeLanguage="http://www.w3.org/2001/XMLSchema" expressionLanguage="http://www.w3.org/1999/XPath" targetNamespace="http://www.activiti.org/test">
  <process id="cherkasy_lmg_415" name="Видача посвідчення мисливця і контрольної картки обліку добутої дичини й порушень правил полювання" isExecutable="true">
    <startEvent id="startevent1" name="Start">
      <extensionElements>
        <activiti:formProperty id="sID_Public_SubjectOrganJoin" name="Оберіть свій район або місто" type="select" default="0"></activiti:formProperty>
        <activiti:formProperty id="bankIdlastName" name="Прізвище " type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdfirstName" name="Ім'я" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdmiddleName" name="По батькові" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdbirthDay" name="Дата народження" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="email" name="Адреса електронної пошти" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="phone" name="Телефон" type="string" default="+380" required="true"></activiti:formProperty>
        <activiti:formProperty id="asType" name="Оберіть тип звернення:" type="enum" required="true">
          <activiti:value id="sEnumCertif" name="Видача Посвідчення мисливця"></activiti:value>
          <activiti:value id="sEnumCard" name="Видача Контрольної картки обліку добутої дичини та порушень правил полювання"></activiti:value>
          <activiti:value id="sEnumCertifDouble" name="Видача дублікату Посвідчення мисливця"></activiti:value>
          <activiti:value id="sEnumCardDouble" name="Видача дублікату Контрольної картки обліку добутої дичини та порушень правил полювання"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="sNameOrgan" name="Назва органа в емейл" type="string"></activiti:formProperty>
        <activiti:formProperty id="sWorkTime" name="Час роботи" type="string"></activiti:formProperty>
        <activiti:formProperty id="sPhoneOrgan" name="Телефон для довідок" type="invisible"></activiti:formProperty>
        <activiti:formProperty id="sAddress" name="Адреса" type="string"></activiti:formProperty>
        <activiti:formProperty id="sMailClerk" name="Пошта держслужбовця" type="invisible"></activiti:formProperty>  
       </extensionElements>
    </startEvent>
    <serviceTask id="servicetask1" name="Завантаження документів" activiti:delegateExpression="#{fileTaskUpload}"></serviceTask>
    <serviceTask id="servicetask2" name="Повідомлення держслужбовця про звернення" activiti:delegateExpression="#{MailTaskWithoutAttachment}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression>tanyazol@gmail.com</activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Нове звернення. "Видача посвідчення мисливця і контрольної картки обліку добутої дичини й порушень правил полювання"]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>[pattern/mail/_common_employee_notify.html]</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <serviceTask id="servicetask3" name="Лист громадянину" activiti:delegateExpression="#{MailTaskWithoutAttachment}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression>${email}</activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Звернення на тему "Видача посвідчення мисливця і контрольної картки обліку добутої дичини й порушень правил полювання"]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>[pattern/mail/_common_header.html]
				&lt;b&gt;Шановний(-а) ${bankIdfirstName} ${bankIdmiddleName}.&lt;/b&gt;&lt;br /&gt;&lt;br /&gt;
				Повідомлення про результат розгляду буде спрямоване на Ваш e-mail.&lt;/p&gt;
				&lt;br /&gt;
				[pattern/mail/_common_signature_start.html]
				${sNameOrgan}&lt;br /&gt;
				[pattern/mail/_common_signature_end.html]
				[pattern/mail/_common_footer.html]</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <userTask id="usertask1" name="Розгляд документів" activiti:candidateGroups="{sID_Public_SubjectOrganJoin}">
      <extensionElements>
        <activiti:formProperty id="clFio" name="Суб’єкт звернення" type="string" default="${bankIdlastName} ${bankIdfirstName} ${bankIdmiddleName}" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdbirthDay" name="Дата народження" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="email" name="Адреса електронної пошти" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdphone" name="Телефон" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="asType" name="Оберіть тип звернення:" type="enum" writable="false">
          <activiti:value id="sEnumCertif" name="Видача Посвідчення мисливця"></activiti:value>
          <activiti:value id="sEnumCard" name="Видача Контрольної картки обліку добутої дичини та порушень правил полювання"></activiti:value>
          <activiti:value id="sEnumCertifDouble" name="Видача дублікату Посвідчення мисливця"></activiti:value>
          <activiti:value id="sEnumCardDouble" name="Видача дублікату Контрольної картки обліку добутої дичини та порушень правил полювання"></activiti:value>
        </activiti:formProperty>
      </extensionElements>
    </userTask>
    <serviceTask id="servicetask4" name="Лист-відмова" activiti:delegateExpression="#{MailTaskWithoutAttachment}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression>${email}</activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Інформація щодо видачі посвідчення мисливця і контрольної картки обліку добутої дичини й порушень правил полювання]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>[pattern/mail/_common_header.html]
			&lt;b&gt;Шановний(-а) ${bankIdfirstName} ${bankIdmiddleName}.&lt;/b&gt;&lt;br&gt;&lt;br&gt;
			&lt;p&gt;На жаль, Ваше звернення щодо ${asType} - відхилене.&lt;/p&gt;
			&lt;p&gt;Причина в наступному: &lt;br&gt;
			${sDecisionComment}&lt;/p&gt;
			&lt;p&gt;Звертаємо увагу, Ви можете заповнити заявку повторно на порталі &lt;a href="https://igov.org.ua/ target="_blank"&gt;iGov.org.ua&lt;/a&gt;&lt;/p&gt;
			 &lt;br&gt; ${sPhoneOrgan} &lt;br&gt;
			[pattern/mail/_common_signature_start.html]
			${sNameOrgan}&lt;/p&gt;
			[pattern/mail/_common_footer.html]</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <serviceTask id="servicetask5" name="Лист-запрошення" activiti:delegateExpression="#{MailTaskWithAttachments}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression>${email}</activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Запрошення до отримання «Посвідчення мисливця і контрольної картки обліку добутої дичини й порушень правил полювання»]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>[pattern/mail/_common_header.html]
			&lt;b&gt;Шановний(-а) ${bankIdfirstName} ${bankIdmiddleName}.&lt;/b&gt;&lt;br /&gt;&lt;br /&gt;
			&lt;p&gt;Ваше звернення щодо ${asType} - успішно розглянуте.&lt;/p&gt;
			&lt;p&gt;Запрошуємо Вас отримати документи за адресою:&lt;br&gt;
			${sAddress}&lt;br&gt;
			&lt;/p&gt;
			При собі необхідно мати:
			1. Для отримання Контрольної картки обліку добутої дичини та порушень правил полювання - Картку за попередній період 
			2. Для отримання Посвідчення мисливця (або дубліката) - паспорт громадянина України та фотокартки (2 шт. 3х4 см)
			&lt;br&gt;${sWorkTime}&lt;br&gt;	
			&lt;br&gt;${sPhoneOrgan}&lt;br&gt;						
			[pattern/mail/_common_signature_start.html]
			${sNameOrgan}&lt;br&gt;
			[pattern/mail/_common_signature_end.html]
			[pattern/mail/_common_feedback.html]
			[pattern/mail/_common_footer.html]</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <boundaryEvent id="boundaryerror1" name="Error" attachedToRef="servicetask1">
      <errorEventDefinition></errorEventDefinition>
    </boundaryEvent>
    <endEvent id="endevent1" name="End"></endEvent>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="servicetask1"></sequenceFlow>
    <sequenceFlow id="flow2" sourceRef="servicetask1" targetRef="servicetask2"></sequenceFlow>
    <sequenceFlow id="flow3" sourceRef="servicetask2" targetRef="servicetask3"></sequenceFlow>
    <sequenceFlow id="flow4" sourceRef="servicetask3" targetRef="usertask1"></sequenceFlow>
    <sequenceFlow id="flow5" sourceRef="usertask1" targetRef="exclusivegateway1"></sequenceFlow>
    <sequenceFlow id="flow6" sourceRef="exclusivegateway1" targetRef="servicetask4"></sequenceFlow>
    <sequenceFlow id="flow7" sourceRef="exclusivegateway1" targetRef="servicetask5"></sequenceFlow>
    <sequenceFlow id="flow8" sourceRef="servicetask5" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow9" sourceRef="servicetask4" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow10" sourceRef="boundaryerror1" targetRef="endevent1"></sequenceFlow>
  </process>
