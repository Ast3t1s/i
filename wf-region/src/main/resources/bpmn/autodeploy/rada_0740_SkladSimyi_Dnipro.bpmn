    <scriptTask id="scripttask1" name="Script Task" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>
var typeMail = execution.getVariable('asLust');
var sum = 15.78;
if (typeMail === 'sEnamLustKur') {
	sum =  36.14;
}
execution.setVariable('sSum', sum);

var name = '';
var mail = '';
var adres = '';
var workTime = '';
switch (asDistrict){
case 'sSamar':
mail = "samara105@ukr.net";
name = "Відділ обліку проживання фізичних осіб управління у сфері державної реєстрації департаменту адміністративних послуг та дозвільних процедур Дніпровської міської ради";
adres = "м. Дніпро, вул. 20-річчя Перемоги, 51 каб. 105";
workTime = "понеділок - пятниця з 9:00 до 19:00";
break;

default:
    alert('ДЛЯ ВАШОГО БУДИНКУ ПОСЛУГУ НАДАТИ НЕ МОЖЛИВО!');
    break;
}
execution.setVariable('sAddress', adres);
execution.setVariable('sWorkDay', workTime);
</script>
    </scriptTask>
