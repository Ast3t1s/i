var per1 = "&lt;li&gt;Документ, що посвідчує особу.&lt;/li&gt;"
var per2 = "&lt;li&gt;Роздруковану заяву, яку ви подавали в електронному вигляді (знаходиться у додатку до листа), засвідчену особистим підписом.&lt;/li&gt;"
var per3 = " "
var res = 'res'
var res2 = 'res2'
if(execution.getVariable('sDocTehVisnovok') != null){
per3 = "&lt;li&gt;Копія матеріалів технічного обстеження житлового приміщення (квартири, житлового будинку), які переводяться в нежитлові підготовлені відповідно до вимог чинного законодавства.&lt;/li&gt;"
}
res = '&lt;ol&gt;'+per1+per2+per3+'&lt;/ol&gt;'
res2 = '&lt;ol&gt;'+per3+'&lt;/ol&gt;'
execution.setVariable('sListDocumentsLetter', res)
execution.setVariable('sListDocumentsPrint', res2)
 </script>
