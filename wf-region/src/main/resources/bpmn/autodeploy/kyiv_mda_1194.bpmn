    <scriptTask id="scripttask1" name="скриптусечка" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>var per1 = ""
var per2 = ""
var per3 = ""
if(execution.getVariable('asTypeChange') == 'sEnumPrepare'){
per1 = "підготовчих робіт"
}
if(execution.getVariable('asTypeChange') == 'sEnumBuild'){
per1 = "будівельних робіт "
}
if(execution.getVariable('saReasonChange5') == 'yes'){
per2 = "Надіслане до Департаменту з питань державного архітектурно-будівельного контролю міста Києва виконавчого органу Київської міської ради (Київської міської державної адміністрації) документи щодо виправлення технічної помилки в декларації про початок виконання"  + per1 + "внесено та зареєстровано у єдиному реєстрі документів, що дають право на виконання підготовчих та будівельних робіт і засвідчують прийняття в експлуатацію закінчених будівництвом об’єктів, відомостей про повернення на доопрацювання, відмову у видачі, скасування та анулювання зазначених документів."
}
if(execution.getVariable('saReasonChange5') != 'yes'){
per2 = "Надіслане до Департаменту з питань державного архітектурно-будівельного контролю міста Києва виконавчого органу Київської міської ради (Київської міської державної адміністрації) електронне повідомлення про внесення змін до декларації про початок виконання" + per1 + "внесено та зареєстровано у єдиному реєстрі документів, що дають право на виконання підготовчих та будівельних робіт і засвідчують прийняття в експлуатацію закінчених будівництвом об’єктів, відомостей про повернення на доопрацювання, відмову у видачі, скасування та анулювання зазначених документів."
}

if(execution.getVariable('saReasonChange5') == 'yes'){
per3 = "Надіслане до Департаменту з питань державного архітектурно-будівельного контролю міста Києва виконавчого органу Київської міської ради (Київської міської державної адміністрації) документи щодо виправлення технічної помилки в декларації про початок виконання"  + per1 + "не зареєстровано у єдиному реєстрі документів, що дають право на виконання підготовчих та будівельних робіт і засвідчують прийняття в експлуатацію закінчених будівництвом об’єктів, відомостей про повернення на доопрацювання, відмову у видачі, скасування та анулювання зазначених документів."
}
if(execution.getVariable('saReasonChange5') != 'yes'){
per3 = "Надіслане до Департаменту з питань державного архітектурно-будівельного контролю міста Києва виконавчого органу Київської міської ради (Київської міської державної адміністрації) електронне повідомлення про внесення змін до декларації про початок виконання" + per1 + "не зареєстровано у єдиному реєстрі документів, що дають право на виконання підготовчих та будівельних робіт і засвідчують прийняття в експлуатацію закінчених будівництвом об’єктів, відомостей про повернення на доопрацювання, відмову у видачі, скасування та анулювання зазначених документів."
}
execution.setVariable('sTextLetterPositive', per2)
execution.setVariable('sTextLetterNegative', per3)
execution.setVariable('sNameWork', per1)</script>
