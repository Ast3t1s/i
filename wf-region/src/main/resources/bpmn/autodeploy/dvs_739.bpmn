<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:activiti="http://activiti.org/bpmn" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC" xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI" typeLanguage="http://www.w3.org/2001/XMLSchema" expressionLanguage="http://www.w3.org/1999/XPath" targetNamespace="http://www.activiti.org/test">
  <process id="dvs_739" name="Форма для подання запиту на отримання публічної інформації від фізичної особи в письмовому вигляді" isExecutable="true">
    <startEvent id="startevent1" name="Start">
      <extensionElements>
        <activiti:formProperty id="sID_Public_SubjectOrganJoin" name="Оберіть територіальний орган ДВС" type="select" default="0" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdlastName" name="Прізвище" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdfirstName" name="Ім'я" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdmiddleName" name="По батькові" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="sPostAddress" name="Поштова адреса" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="phone" name="Контактний телефон" type="string" default="+380" required="true"></activiti:formProperty>
        <activiti:formProperty id="email" name="Ваш e-mail" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="sLabel1" name="Суть запиту" type="label" default=" "></activiti:formProperty>
        <activiti:formProperty id="sDocument" name="Вид, назва, реквізити, зміст документа, що запитується" type="textArea"></activiti:formProperty>
        <activiti:formProperty id="sLabel2" name="або" type="label" default=" "></activiti:formProperty>
        <activiti:formProperty id="sInfo" name="Загальний опис інформації, що запитується" type="textArea"></activiti:formProperty>
        <activiti:formProperty id="asGet" name="Спосіб отримання відповіді на запит" type="enum" required="true">
          <activiti:value id="sEnumPost" name="поштою"></activiti:value>
          <activiti:value id="sEnumFax" name="факсом"></activiti:value>
          <activiti:value id="sEnumEmail" name="електронною поштою"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="sFax" name="Номер факсу" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="sPhoneOrgan" name="Телефон для довідок" type="invisible"></activiti:formProperty>
        <activiti:formProperty id="sNameOrgan" name="Назва органа" type="invisible"></activiti:formProperty>
        <activiti:formProperty id="sAddress" name="Адреса" type="invisible"></activiti:formProperty>
        <activiti:formProperty id="sWorkTime" name="Час роботи" type="invisible"></activiti:formProperty>
        <activiti:formProperty id="sMailClerk" name="Пошта держслужбовця" type="invisible"></activiti:formProperty>
        <activiti:formProperty id="markers" name="Факс" type="markers" default="{&quot;motion&quot;:{&quot;ShowFieldsOnCondition_1&quot;:{&quot;aField_ID&quot;:[&quot;sFax&quot;],&quot;asID_Field&quot;:{&quot;sClient&quot;:&quot;asGet&quot;},&quot;sCondition&quot;:&quot;[sClient] == 'sEnumFax'&quot;}}}"></activiti:formProperty>
      </extensionElements>
    </startEvent>
    <serviceTask id="servicetask1" name="Лист службовцю" activiti:delegateExpression="#{MailTaskWithoutAttachment}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression>tanyazol@gmail.com</activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Нове звернення. Запит на отримання публічної інформації.]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>[pattern/mail/_common_employee_notify.html]</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <serviceTask id="servicetask2" name="Лист клієнту" activiti:delegateExpression="#{MailTaskWithoutAttachment}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression>${email}</activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Запит на отримання публічної інформації.]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>[pattern/mail/_common_header.html]
			&lt;b&gt;Шановний(-а) ${bankIdfirstName} ${bankIdmiddleName}.&lt;/b&gt;&lt;br /&gt;&lt;br /&gt;
			&lt;p&gt; Ваше звернення щодо запиту на отримання публічної інформації було зареєстровано у системі.&lt;br/&gt;
			Повідомлення про результат послуги буде спрямоване на Ваш e-mail.&lt;/p&gt;
			&lt;br /&gt;
			${sPhoneOrgan}&lt;br /&gt;
			[pattern/mail/_common_signature_start.html]
			${sNameOrgan}&lt;br /&gt;
			[pattern/mail/_common_signature_end.html]
			[pattern/mail/_common_footer.html]</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <userTask id="usertask1" name="Надання послуги" activiti:candidateGroups="dvs_${sID_Public_SubjectOrganJoin}">
      <extensionElements>
        <activiti:formProperty id="sNameOrgan" name="Назва органа" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="clFio" name="Заявник:" type="string" default="${bankIdlastName} ${bankIdfirstName} ${bankIdmiddleName}" writable="false"></activiti:formProperty>
        <activiti:formProperty id="phone" name="Контактний телефон" type="string" default="+380" writable="false"></activiti:formProperty>
        <activiti:formProperty id="email" name="e-mail" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="sPostAddress" name="Поштова адреса" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="sLabel1" name="Суть запиту" type="label" default=" "></activiti:formProperty>
        <activiti:formProperty id="sDocument" name="Вид, назва, реквізити, зміст документа, що запитується" type="textArea" writable="false"></activiti:formProperty>
        <activiti:formProperty id="sInfo" name="Загальний опис інформації, що запитується" type="textArea" writable="false"></activiti:formProperty>
        <activiti:formProperty id="asGet" name="Спосіб отримання відповіді на запит" type="enum" writable="false">
          <activiti:value id="sEnumPost" name="Поштою"></activiti:value>
          <activiti:value id="sEnumFax" name="Факсом"></activiti:value>
          <activiti:value id="sEnumEmail" name="Електронною поштою"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="sFax" name="Номер факсу" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="asStatus" name="Статус запиту" type="enum" required="true">
          <activiti:value id="sEnumDeny" name="Відмова заявнику у видачі довідки"></activiti:value>
          <activiti:value id="sEnumEmail" name="Інформація готова, відправити email"></activiti:value>
          <activiti:value id="sEnumPostFax" name="Інформація готова, відправлено факс або поштове відправлення"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="sComment" name="Якщо ви відмовляєте заявнику у наданні послуги, обов'язково вкажіть у цьому полі: ПІБ та посаду особи, відповідальної за розгляд запиту розпорядником інформації, дату, мотивовану підставу та порядок оскарження відмови" type="textArea"></activiti:formProperty>
        <activiti:formProperty id="file1" name="Завантажте скановану копію довідки, з накладеним ЕЦП (електронно-цифровим підписом)" type="file"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <serviceTask id="servicetask3" name="Лист з інфо" activiti:delegateExpression="#{MailTaskWithAttachments}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression>${email}</activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Запит на отримання публічної інформації.]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>[pattern/mail/_common_header.html]
			&lt;b&gt;Шановний(-а) ${bankIdfirstName} ${bankIdmiddleName}.&lt;/b&gt;&lt;br /&gt;&lt;br /&gt;
			&lt;p&gt;Відповідь на Ваш  запит на отримання публічної інформації - готова&lt;br /&gt;	&lt;/p&gt;
			Скан-копію відповіді прикріплено до цього листа.&lt;br /&gt;
			Оригінал відповіді можете отримати за адресою:
			&lt;br /&gt;
			&lt;br /&gt;
			${sAddress}&lt;br /&gt;
			${sWorkTime}&lt;br /&gt;
			&lt;br /&gt;
			${sPhoneOrgan}&lt;br /&gt;
			[pattern/mail/_common_signature_start.html]
			${sNameOrgan}&lt;br /&gt;
			[pattern/mail/_common_signature_end.html]
			[pattern/mail/_common_feedback.html]
			[pattern/mail/_common_footer.html]</activiti:expression>
        </activiti:field>
        <activiti:field name="saAttachmentsForSend">
          <activiti:expression>${file1}</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <serviceTask id="servicetask4" name="Лист-відмова" activiti:delegateExpression="#{MailTaskWithoutAttachment}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression>${email}</activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Запит на отримання публічної інформації.]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>[pattern/mail/_common_header.html]
			&lt;b&gt;Шановний(-а) ${bankIdfirstName} ${bankIdmiddleName}.&lt;/b&gt;&lt;br /&gt;&lt;br /&gt;
			На жаль, Ваше звернення щодо запиту на отримання публічної інформації - відхилене.&lt;/p&gt;
			&lt;p&gt;Причина в наступному: &lt;/p&gt;
			&lt;p&gt;${sComment}&lt;/p&gt;
			&lt;p&gt;Звертаємо увагу, Ви можете заповнити заявку повторно на порталі &lt;a href="https://igov.org.ua/ target="_blank"&gt;iGov.org.ua&lt;/a&gt;&lt;/p&gt;
			&lt;br /&gt;
			${sPhoneOrgan}&lt;br /&gt;
			[pattern/mail/_common_signature_start.html]
			${sNameOrgan}&lt;br /&gt;
			[pattern/mail/_common_signature_end.html]
			[pattern/mail/_common_footer.html]</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <serviceTask id="servicetask5" name="Відправлено поштою\факс" activiti:delegateExpression="#{MailTaskWithoutAttachment}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression>${email}</activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Запит на отримання публічної інформації.]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression>[pattern/mail/_common_header.html]
			&lt;b&gt;Шановний(-а) ${bankIdfirstName} ${bankIdmiddleName}.&lt;/b&gt;&lt;br /&gt;&lt;br /&gt;
			&lt;p&gt;
			Відповідь на Ваш  запит на отримання публічної інформації - готова&lt;br /&gt;
			&lt;/p&gt;
			
			&lt;p&gt;Згідно до Вашої заяви, відповідь відправлено enum{[asGet]}&lt;br /&gt;	&lt;/p&gt;
			Оригінал відповіді можете отримати за адресою:
			&lt;br /&gt;
			&lt;br /&gt;
			${sAddress}&lt;br /&gt;
			${sWorkTime}&lt;br /&gt;
			&lt;br /&gt;
			${sPhoneOrgan}&lt;br /&gt;
			[pattern/mail/_common_signature_start.html]
			${sNameOrgan}&lt;br /&gt;
			[pattern/mail/_common_signature_end.html]
			[pattern/mail/_common_feedback.html]
			[pattern/mail/_common_footer.html]</activiti:expression>
        </activiti:field>
        
        </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="servicetask1"></sequenceFlow>
    <sequenceFlow id="flow2" sourceRef="servicetask1" targetRef="servicetask2"></sequenceFlow>
    <sequenceFlow id="flow3" sourceRef="servicetask2" targetRef="usertask1"></sequenceFlow>
    <sequenceFlow id="flow4" sourceRef="usertask1" targetRef="exclusivegateway1"></sequenceFlow>
    <sequenceFlow id="flow5" name="Email" sourceRef="exclusivegateway1" targetRef="servicetask3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${asStatus=='sEnumEmail'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow6" name="Deny" sourceRef="exclusivegateway1" targetRef="servicetask4">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${asStatus=='sEnumDeny'}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow7" sourceRef="servicetask3" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow8" sourceRef="servicetask4" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow9" name="PostFax" sourceRef="exclusivegateway1" targetRef="servicetask5">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${asStatus=='sEnumPostFax'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow10" sourceRef="servicetask5" targetRef="endevent1"></sequenceFlow>
  </process>
