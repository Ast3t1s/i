<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:activiti="http://activiti.org/bpmn" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC" xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI" typeLanguage="http://www.w3.org/2001/XMLSchema" expressionLanguage="http://www.w3.org/1999/XPath" targetNamespace="http://www.activiti.org/test">
  <process id="infrastr_845" name="Оформлення посвідчення члена екіпажу" isExecutable="true">
    <startEvent id="startevent1" name="Start" activiti:initiator="initiator">
      <extensionElements>
        <activiti:formProperty id="bankIdlastName" name="Прізвище" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdfirstName" name="Ім'я" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdmiddleName" name="По батькові" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="phone" name="Ваш контактний  телефон" type="string" default="+380" required="true"></activiti:formProperty>
        <activiti:formProperty id="email" name="Ваш e-mail; На цей e-mail Ви отримаєте результат обробки Вашого звернення" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="sCompName" name="Назва авіакомпанії" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="asCount" name="Кількість громадян, яким необхідно оформити посвідчення;Максимальна кількість для однієї заявки - 10 осіб" type="enum" required="true" default="sOne">
          <activiti:value id="sOne" name="один"></activiti:value>
          <activiti:value id="sTwo" name="два"></activiti:value>
          <activiti:value id="sThree" name="три"></activiti:value>
          <activiti:value id="sFour" name="чотири"></activiti:value>
          <activiti:value id="sFive" name="п'ять"></activiti:value>
          <activiti:value id="sSix" name="шість"></activiti:value>
          <activiti:value id="sSeven" name="сім"></activiti:value>
          <activiti:value id="sEight" name="вісім"></activiti:value>
          <activiti:value id="sNine" name="дев'ять"></activiti:value>
          <activiti:value id="sTen" name="десять"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="nDocComp" name="Подання щодо оформлення посвідчення" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="lable1" name="Зверніть увагу" type="label" default="До пакету докуменів входять копія паспорта громадянина України (всі заповнені сторінки) та копія посвідчення члена екіпажу (за наявності)"></activiti:formProperty>
        <activiti:formProperty id="nManDoc1" name="Пакет документів по першому громадянину" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="nManDoc2" name="Пакет документів по другому громадянину" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="nManDoc3" name="Пакет документів по третьому громадянину" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="nManDoc4" name="Пакет документів по четвертому громадянину" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="nManDoc5" name="Пакет документів по п'ятому громадянину" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="nManDoc6" name="Пакет документів по шостому громадянину" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="nManDoc7" name="Пакет документів по сьомому громадянину" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="nManDoc8" name="Пакет документів по восьмому громадянину" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="nManDoc9" name="Пакет документів по дев'ятому громадянину" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="nManDoc10" name="Пакет документів по десятому громадянину" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="markers1" name="hide fields" type="markers" default="${markerService.loadFromFile('infrastr/air_1.json')}"></activiti:formProperty>
      </extensionElements>
    </startEvent>
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="servicetask1"></sequenceFlow>
    <serviceTask id="servicetask1" name="Повідомлення про нове звернення" activiti:delegateExpression="#{MailTaskWithoutAttachment}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression><![CDATA[zhigan.roman@gmail.com]]></activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:expression><![CDATA[Нова заявка на оформлення посвідчення члена екіпажу]]></activiti:expression>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression><![CDATA[[pattern/mail/_common_employee_notify.html]]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow2" sourceRef="servicetask1" targetRef="servicetask2"></sequenceFlow>
    <serviceTask id="servicetask2" name="Повідомлення про успішну реєстрацію звернення" activiti:delegateExpression="#{MailTaskWithoutAttachment}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${email}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:expression><![CDATA[Оформлення посвідчення члена екіпажу]]></activiti:expression>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression><![CDATA[[pattern/mail/_common_header.html]
		    					<h3>Шановний(-а) ${bankIdfirstName} ${bankIdmiddleName}.</h3>
								<p>
            						Ваша заявка на видачу посвідчення члена екіпажу успішно зареєстрована №[sID_Order].<br>
									Спеціалісти Державної авіаційної служби України повинні дати відповідь найближчим часом.
                                </p>
                                [pattern/mail/_common_signature_start.html]
								Державна авіаційна служби України,<br>
								[pattern/mail/_common_signature_end.html]
								[pattern/mail/_common_footer.html]]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow3" sourceRef="servicetask2" targetRef="usertask1"></sequenceFlow>
    <userTask id="usertask1" name="Перевірка вхідних даних" activiti:candidateGroups="infr_air_1">
      <extensionElements>
        <activiti:formProperty id="sFio" name="Автор звернення" type="string" default="${bankIdlastName} ${bankIdfirstName} ${bankIdmiddleName}" writable="false"></activiti:formProperty>
        <activiti:formProperty id="phone" name="Контактний  телефон" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="email" name="Е-mail" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="sCompName" name="Назва авіакомпанії" type="string" writable="false"></activiti:formProperty>
        <activiti:formProperty id="asCount" name="Кількість громадян, яким необхідно оформити посвідчення;Максимальна кількість для однієї заявки - 10 осіб" type="enum" writable="false">
          <activiti:value id="sOne" name="один"></activiti:value>
          <activiti:value id="sTwo" name="два"></activiti:value>
          <activiti:value id="sThree" name="три"></activiti:value>
          <activiti:value id="sFour" name="чотири"></activiti:value>
          <activiti:value id="sFive" name="п'ять"></activiti:value>
          <activiti:value id="sSix" name="шість"></activiti:value>
          <activiti:value id="sSeven" name="сім"></activiti:value>
          <activiti:value id="sEight" name="вісім"></activiti:value>
          <activiti:value id="sNine" name="дев'ять"></activiti:value>
          <activiti:value id="sTen" name="десять"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="sManPositive" name="Прізвища та ініціали осіб, які готові для видачі посвідчення" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="sManNegative" name="Прізвища та ініціали осіб, по яких треба внести зауваження" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="comment" name="Додайте у цьому полі зміст зауважень, або парметри необхадна для подання заявки" type="textArea" default=" "></activiti:formProperty>
        <activiti:formProperty id="nBill" name="Рахунок для сплати" type="file"></activiti:formProperty>
        <activiti:taskListener event="create" delegateExpression="${fileTaskUploadListener}"></activiti:taskListener>
        <activiti:taskListener event="create" delegateExpression="${fileTaskInheritance}">
          <activiti:field name="aFieldInheritedAttachmentID">
            <activiti:expression><![CDATA[${nDocComp},${nManDoc1},${nManDoc2},${nManDoc3},${nManDoc4},${nManDoc5},${nManDoc6},${nManDoc7},${nManDoc8},${nManDoc9},${nManDoc10}]]></activiti:expression>
          </activiti:field>
        </activiti:taskListener>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow4" sourceRef="usertask1" targetRef="servicetask4"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway3" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow5" name="${decide == 'reject'}" sourceRef="exclusivegateway3" targetRef="servicetask3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${decide == 'reject'}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="servicetask3" name="Повідомлення про відмову" activiti:delegateExpression="#{MailTaskWithoutAttachment}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${email}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Оформлення посвідчення члена екіпажу]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression><![CDATA[[pattern/mail/_common_header.html]
		    <h3>Шановний(-а) ${bankIdfirstName} ${bankIdmiddleName}.</h3>
			<p>
				На жаль, ми змушені відмовити Вам у наданні послуги за заявкою №[sID_Order].
			</p>
			<p>
			Коментар державного службовця:<br>
			${comment}
			</p>
			За необхідності Ви можете подати ще одну заявку на порталі <a href="https://igov.org.ua/" target="_blank">igov.org.ua</a></p>
			[pattern/mail/_common_signature_start.html]
			Державна авіаційна служби України.</p>
			[pattern/mail/_common_footer.html]]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow6" sourceRef="servicetask3" targetRef="endevent5"></sequenceFlow>
    <endEvent id="endevent5" name="End"></endEvent>
    <sequenceFlow id="flow7" name="decide == 'accept'" sourceRef="exclusivegateway3" targetRef="servicetask4">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${decide == 'accept'}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="servicetask4" name="Відправка відповіді на запит користувачу" activiti:delegateExpression="#{MailTaskWithAttachments}">
      <extensionElements>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${email}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:string><![CDATA[Оформлення посвідчення члена екіпажу]]></activiti:string>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression><![CDATA[[pattern/mail/_common_header.html]
			<h3>Шановний(-а) ${bankIdfirstName} ${bankIdmiddleName}.</h3>
			<p>Ваша заявка №[sID_Order] на видачу посвідчення члена екіпажу прийнята в роботу.<br>
			В додатках до листа ви знайдете рахунок для сплати державного збору.
			</p> 
			<p>
			Для того щоб закінчити процедуру подання документів для оформлення посвідчення члена екіпажу необхідно:<br>
			<ul>
			<li>Сплатити державний збір (в додатках до листа ви знайдете рахунок) та отримати квитанцію про сплату.</li>
			<li>Протягом 3-х робочіх днів відвідати Державіаслужбу за адресою: <b>м. Київ, пр. Перемоги, 14</b> та принести оригінал подання та завірені копії документів членів екіпажу.</li>
			</ul>
			</p> 
			<p>Докумети в порядку у ${sManPositive}
			</p> 
			<p>Наявні помилки в документах у ${sManNegative}
			${comment}
			</p>
            [pattern/mail/_common_signature_start.html]
			Державна авіаційна служби України,<br>
			[pattern/mail/_common_signature_end.html]
			[pattern/mail/_common_footer.html]]]></activiti:expression>
        </activiti:field>
        <activiti:field name="saAttachmentsForSend">
          <activiti:expression><![CDATA[${nBill}]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow44" sourceRef="servicetask4" targetRef="endevent6"></sequenceFlow>
    <endEvent id="endevent6" name="End"></endEvent>
  </process>
