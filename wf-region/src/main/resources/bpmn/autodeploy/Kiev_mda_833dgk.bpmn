        <activiti:formProperty id="map2" name="Посилання на карту для позначеня меж:" type="label" default="&lt;a href=&quot;http://map.land.gov.ua/kadastrova-karta&quot;&gt;Публічна Кадастрова мапа за цим посиланням&lt;/a&gt;"></activiti:formProperty>
