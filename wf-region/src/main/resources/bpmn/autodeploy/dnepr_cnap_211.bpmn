    <scriptTask id="scripttask1" name="Script Task" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>var sum = 16.09;
if ((execution.getVariable('asLust')) === 'sEnamLustKur') {
	sum =  36.45;
}
execution.setVariable('sSum', sum);      

var num1 = [ "otr", "orend", "podatok"]
var num2 = [ "Для отримання земельної ділянки у власність", "Для оформлення договору оренди", "Для подачі в податкову для нарахування податку на землю"]
for (var i = 0; i&lt;3; i++){
var per5 = num1[i]
if(execution.getVariable('goal') == per5){
var per6 = num2[i]
break}}
execution.setVariable('CLgoal', per6)

var per10 = 'no_dgk'
if(execution.getVariable('sID_Public_SubjectOrganJoin') == 'magdal')
{var per10 = 'yes_dgk'}

execution.setVariable('sflag_dgk', per10)
      </script>
